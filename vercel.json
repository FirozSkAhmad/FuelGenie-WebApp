{
  "redirects": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "dashboard.fuelgenieoffice.com" }],
      "destination": "https://dashboard.fuelgenieoffice.com/:path*",
      "permanent": true
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
